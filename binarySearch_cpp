#include <iostream>
using namespace std;

int binarySearch(int arr[], int n, int key){
    
    int s=0, e=n-1;
    int mid = s + (e-s)/2;
    
    while(mid<=e){
        if(arr[mid] == key){
            return mid;
        }
        else if(key < arr[mid]){
            e=mid-1;
        }
        else{
            s=mid+1;
        }
        mid = s + (e-s)/2;
    }
    return -1;
}

int main() {

    int arr[]={1,3,5,7,9,11,33,56,78,99};
    
    cout<<binarySearch(arr, 10, 3);
    return 0;
}
